{
  "name": "smart-meter-client",
  "version": "1.0.0",
  "description": "智能电表数据展示前端应用",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "node scripts/build.js development",
    "build:prod": "node scripts/build.js production",
    "build:analyze": "node scripts/build.js analyze",
    "build:pwa": "node scripts/build.js pwa",
    "preview": "vite preview",
    "preview:dist": "vite preview --port 4173",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 2000",
    "lint:fix": "eslint . --fix",
    "check": "npm run lint && npm run build",
    "analyze": "npm run build:analyze",
    "size-check": "npm run build:prod && node scripts/build.js analyze",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@codemirror/lang-sql": "^6.9.1",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@tanstack/query-sync-storage-persister": "^5.83.1",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.85.0",
    "@tanstack/react-query-persist-client": "^5.85.0",
    "@uiw/react-codemirror": "^4.25.1",
    "antd": "^5.12.8",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "lodash-es": "^4.17.21",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "globals": "^16.3.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.2"
  }
}
